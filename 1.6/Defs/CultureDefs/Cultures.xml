    <allowedPlaceTags>
      <li>OriginTribal</li>
    </allowedPlaceTags>
    <iconPath>MapIcons/CastleCivil</iconPath>
    <iconColor>(0.8, 0.8, 0.8)</iconColor>
    <preferredWeaponClasses>
      <noble>MeleePiercer</noble>
      <despised>Ultratech</despised>
    </preferredWeaponClasses>
    <styleItemTags>
