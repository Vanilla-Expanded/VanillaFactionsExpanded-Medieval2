				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.1</setMax>
					</li>
				</capMods>
			</li>
			<li Name="potent">
				<label>final</label>
				<minSeverity>0.9</minSeverity>
				<capMods>
